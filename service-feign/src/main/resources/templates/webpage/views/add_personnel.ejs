<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>人员登记</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Pace -->
    <link rel="stylesheet" href="../../plugins/pace-master/themes/blue/pace-theme-flash.css">
    <link rel="stylesheet" href="../../p">
    <script type="text/javascript" src="../../plugins/pace-master/pace.min.js"></script>
    <link rel="stylesheet" href="../../bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="../../dist/css/font-awesome.min.css">
    <link rel="stylesheet" href="../../dist/css/ionicons.min.css">
    <link rel="stylesheet" href="../../dist/css/AdminLTE.min.css">
    <link rel="stylesheet" href="../../dist/css/skins/all-skins.min.css">
    <link rel="stylesheet" href="../../dist/css/datepicker.css">
    <link rel="stylesheet" href="../../dist/css/jquery-ui.css">
    <!-- daterange picker -->
    <link rel="stylesheet" href="../../bower_components/bootstrap-daterangepicker/daterangepicker.css">
    <!-- bootstrap datepicker -->
    <link rel="stylesheet" href="../../bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
</head>

<body class="hold-transition skin-blue sidebar-mini">

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            <small></small>
        </h1>
        <ol class="breadcrumb">
            <li>
                <a href="#">
                    <i class="fa fa-dashboard"></i> 人员管理</a>
            </li>
            <li>
                <a href="#">人员登记</a>
            </li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

        <div class="box box-info">
            <div class="box-header with-border">
                <h3 class="box-title">
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit;">添加人员</font>
                    </font>
                </h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form action="#" class="form-horizontal" method="POST" style="width:1200px;margin:0 auto;">
                <div class="box-body">
                <div class="borders box box-default box-body" >  
                    <div class="form-group col-md-12">
                        <font style="vertical-align: inherit;font-size:18px;"><b>个人信息：</b></font>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="username" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">姓名：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="username" placeholder="请输入姓名">
                        </div>
                    </div>
                    
                    <div class="form-group col-md-4">
                        <label for="sex" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">性别：</font>
                            </font>
                        </label>
                        <div class="col-sm-8" >
                            <label for="mens"><input id="men" type="radio" value="0" name="chose">男</label>
                            &nbsp;&nbsp;&nbsp;&nbsp;
                            <label for="womens"><input id="women" type="radio" value="1" name="chose">女</label>
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="sex" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">出生地点：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="sex" placeholder="请输入出生地点">
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="nation" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">民族：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="nation" placeholder="请输入民族">
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="native-place" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">籍贯：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="native-place" placeholder="请输入籍贯">
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="healthy" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">健康状况：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right" id="healthy" placeholder="请输入健康状况">
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="datebirth" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">出生日期：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right briths" id="datebirth" autocomplete="off" placeholder="请输入出生日期">
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="join-party" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">入党时间：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right briths" id="join-party"  autocomplete="off" placeholder="请输入入党时间">
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label for="data-work" class="col-sm-5 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">参加工作时间：</font>
                            </font>
                        </label>
                        <div class="col-sm-7">
                            <input type="text" class="form-control pull-right briths" id="data-work" autocomplete="off"  placeholder="请输入参加工作时间">
                        </div>
                    </div>
                    </div>
                    <div class="borders box box-default box-body" >
                    <div class="form-group col-md-12">
                        <font style="vertical-align: inherit;font-size:18px;"><b>学历信息：</b></font>
                    </div>

                    <div class="form-group col-md-6">
                        <label for="education" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">学历：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right" id="education" placeholder="请输入学历">
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label for="school" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">毕业学校：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right" id="school" placeholder="请输入毕业学校">
                        </div>
                    </div>
                    
                    <div class="form-group col-md-6">
                        <label for="incumbent-post" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">现任职务：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right" id="incumbent-post" placeholder="请输入现任职务">
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label for="major" class="col-sm-4 control-label">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">毕业专业：</font>
                            </font>
                        </label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control pull-right" id="major" placeholder="请输入毕业专业">
                        </div>
                    </div>
                    </div>

                    <div class="borders box box-default box-body" >
                        <div class="form-group col-md-12">
                            <font style="vertical-align: inherit;font-size:18px;"><b>家庭信息：</b></font>
                        </div>

                        <div class="form-group col-md-4">
                            <label for="relatives" class="col-sm-4 control-label">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">亲属姓名：</font>
                                </font>
                            </label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="relatives" placeholder="请输入亲属姓名">
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label for="relationship" class="col-sm-4 control-label">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">亲属关系：</font>
                                </font>
                            </label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="relationship" placeholder="请输入亲属关系">
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label for="relatives-tel" class="col-sm-4 control-label">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">联系方式：</font>
                                </font>
                            </label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="relatives-tel" placeholder="请输入亲属联系方式">
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label for="relatives-occupation" class="col-sm-4 control-label">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">亲属职业：</font>
                                </font>
                            </label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="relatives-occupation" placeholder="请输入亲属职业">
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label for="relatives-native" class="col-sm-4 control-label">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">亲属民族：</font>
                                </font>
                            </label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="relatives-native" placeholder="请输入亲属民族">
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
                <div class="box-footer" style="text-align:center;">
                    <button type="button" class="btn btn-default ">
                        <font style="vertical-align: inherit;">
                            <font style="vertical-align: inherit;">取消</font>
                        </font>
                    </button>
                    <button type="submit" class="btn btn-info">
                        <font style="vertical-align: inherit;">
                            <font style="vertical-align: inherit;">保存</font>
                        </font>
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
        </div>

    </section>

    <script src="../../plugins/jQuery/jquery-2.2.3.min.js"></script>
    <script src="../../bootstrap/js/bootstrap.min.js"></script>
    <script src="../../plugins/fastclick/fastclick.js"></script>
    <script src="../../dist/js/app.min.js"></script>
    <script src="../../dist/js/demo.js"></script>
    <!-- date-range-picker -->
    <script src="../../bower_components/moment/min/moment.min.js"></script>
    <script src="../../bower_components/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- bootstrap datepicker -->
    <script src="../../bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.js"></script>
    <script src="../../bower_components/jquery-ui/ui/i18n/datepicker-zh-CN.js"></script>
    <script src="../public/js/dates.js"></script>
</body>
<script>

    $(function () {
        $('.briths').datepicker({
            autoclose: true,
            dateFormat: "dd-mm-yy",
        })
      
        
    })

</script>

</html>